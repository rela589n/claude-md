{
  "permissions": {
    "allow": [
      "Bash(find:*)",
      "WebSearch"
    ],
    "defaultMode": "default"
  },
  "autoUpdatesChannel": "stable",
  "hooks": {
    "Notification": [
      {
        "matcher": "permission_prompt",
        "hooks": [
          {
            "type": "command",
            "command": "paplay /usr/share/sounds/freedesktop/stereo/complete.oga"
          }
        ]
      }
    ]
  }
}
